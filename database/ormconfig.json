{
  "type": "sqlite",
  "synchronize": true,
   "logging": false,
   "database": "../src/assets/data/database.sqlite",

   "entities": [
      "src/domain/entity/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/domain/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
