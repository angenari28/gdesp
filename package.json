{
  "name": "gdesp",
  "version": "1.0.1",
  "main": "./main/main.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage --no-cache",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "npm run clean && ng build --base-href ./ && electron .",
    "electron-forge": "npm run clean && ng build --base-href ./ && electron-forge start",
    "package": "ng b --prod && electron-forge package",
    "asar-pack": "asar pack ./out/gdesp-win32-x64/resources/app ./out/gdesp-win32-x64/resources/app.asar",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "clean": "del-cli dist",
    "postinstall": "ngcc"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "./src/assets/images/financas.icns"
      },
      "electronWinstallerConfig": {
        "name": "gdesp"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Andre Genari",
        "name": "angenari28/gdesp"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "gdesp"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ng-select/ng-select": "^4.0.4",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.4",
    "electron": "^9.3.5",
    "electron-compile": "^6.4.4",
    "g": "^2.0.1",
    "ini": "^2.0.0",
    "jquery": "^3.5.1",
    "ng-select": "^1.0.2",
    "ng2-charts": "^2.4.2",
    "ngx-bootstrap": "^5.6.2",
    "ngx-electron": "^2.2.0",
    "ngx-mask": "^9.1.4",
    "popper.js": "^1.16.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "sqlite3": "^4.2.0",
    "tslib": "^1.14.1",
    "typeorm": "^0.2.29",
    "uglify-js": "^3.12.2",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@briebug/jest-schematic": "^3.1.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.7",
    "asar": "^3.0.3",
    "codelyzer": "^5.2.2",
    "electron-forge": "^5.2.4",
    "electron-packager": "^15.1.0",
    "electron-prebuilt-compile": "8.2.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 85,
        "functions": 75,
        "lines": 80
      }
    }
  }
}
