{
  "name": "gdesp",
  "version": "1.0.0",
  "main": "./main/main.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "npm run clean && ng build --base-href ./ && electron .",
    "electron-forge": "npm run clean && ng build --base-href ./ && electron-forge start",
    "package": "ng build && electron-forge package",
    "asar-pack": "asar pack ./out/gdesp-win32-x64/resources/app ./out/gdesp-win32-x64/resources/app.asar",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "clean": "del-cli dist"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "gdesp"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "gdesp"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@ng-select/ng-select": "^3.7.3",
    "bootstrap": "4.1.1",
    "chart.js": "^2.9.3",
    "electron": "^9.1.0",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "ng-select": "^1.0.2",
    "ng2-charts": "^2.3.2",
    "ngx-bootstrap": "^5.6.1",
    "ngx-electron": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "sqlite3": "^4.2.0",
    "tslib": "^1.13.0",
    "typeorm": "^0.2.24",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.47",
    "asar": "^3.0.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codelyzer": "^5.2.2",
    "electron-forge": "^5.2.4",
    "electron-packager": "^14.2.1",
    "electron-prebuilt-compile": "8.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
